<div id="logins" class="auth-container container">
  <div class="auth-inner-container">
  <% if request.path.include? "visitor" %>
    <%= render "visitor" %>
  <% else %>
    <%= render "#{institution_from_code(params[:auth_type]).views["login"]["primary"]}" %>
  <% end %>
  <% if params[:auth_type] == "bobst" %>
    <%= render "devise/sessions/help/#{params[:institution]}_help" rescue render "devise/sessions/help/help" %>
  <% else %>
    <%= render "devise/sessions/help/#{params[:auth_type]}_help" rescue render "devise/sessions/help/help" %>
  <% end %>
  </div>
</div>
