<div id="logins" class="auth-container container">
  <div class="auth-inner-container">
  <% if request.path.include? "visitor" %>
    <%= render "visitor" %>
  <% else %>
    <%= render "#{institution_from_code(params[:auth_type]).views["login"]["primary"]}" %>
  <% end %>
  <h2 class="help"><%= fa_icon "question-circle 2x", class: "circle" %><span class="help_text"><%= t('application.auth_footer_html', link: link_to('Ask a Librarian', 'http://library.nyu.edu/ask', target: '_blank')) %></span></h2>
  </div>
</div>
