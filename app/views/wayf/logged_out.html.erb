<div id="logins" class="container options-container">
  <div class="inner-container">
    <div class="row">
      <h1><%= t('application.logged_out') %></h1>
    </div>
    <div class="row logged_out_text">
      <p>Exit your browser to log out everywhere - keep other people from logging into your account</p>
    </div>
    <div class="row">
      <h2 class="help">
        <%= fa_icon "question-circle 2x", class: "circle" %>
        <span class="help_text">
          <%= link_to t('application.logged_out_footer_html'), 'http://library.nyu.edu/ask', target: :_blank %>
        </span>
      </h2>
    </div>
  </div>
</div>
