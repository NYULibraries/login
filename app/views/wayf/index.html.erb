<div id="logins" class="container options-container">
  <div class="inner-container">
    <% if request.path.include? "visitor" %>
      <%= render "visitor" %>
    <% else %>
      <%= render "#{institution.code.to_s.downcase}" %>
    <% end %>
    <div class="row">
      <h2 class="help"><%= fa_icon "question-circle 2x", class: "circle" %><span class="help_text"><%= t('application.footer_html', link: link_to('Ask a Librarian', 'http://library.nyu.edu/ask', target: '_blank')) %></span></h2>
    </div>
  </div>
</div>
